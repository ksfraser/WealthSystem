set grid
show grid
set title "Normal Distribution"
show title
set xlabel "3 Sigma Standard Deviation"
show xlabel
set ylabel "Normalized Frequency"
show ylabel
plot "tsbrownian.tsnormal" with lines, "tsbrownian.tsnormal-f" with steps
pause -1
